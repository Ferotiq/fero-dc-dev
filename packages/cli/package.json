{
  "name": "@ferod/create",
  "version": "0.0.0",
  "description": "Create CLI for Ferod",
  "types": "./dist/index.d.ts",
  "author": "Evan <evanrayweigel@outlook.com>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "ferod": "bin/ferod.mjs",
    "create-ferod": "bin/ferod.mjs"
  },
  "scripts": {
    "build": "tsup",
    "dev": "ts-node-dev src/index.ts",
    "test": "cd test && npx ferod -y",
    "pretty": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "pretty-fix": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prepare": "pnpm build && pnpm lint && pnpm pretty"
  },
  "dependencies": {
    "commander": "10.0.1",
    "discord.js": "14.11.0",
    "esm": "3.2.25",
    "fs-extra": "11.1.1",
    "inquirer": "9.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.3"
  }
}
