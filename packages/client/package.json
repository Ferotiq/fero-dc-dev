{
  "name": "@ferod/client",
  "description": "An extension on Discord.JS that makes it simpler and safer to create Discord bots.",
  "version": "0.1.3",
  "author": "Evan <evanrayweigel@outlook.com>",
  "repository": "https://github.com/ferotiq/ferod",
  "bugs": {
    "url": "https://github.com/ferotiq/ferod/issues"
  },
  "homepage": "https://github.com/ferotiq/ferod#readme",
  "license": "MIT",
  "engines": {
    "node": "18.16.0",
    "pnpm": "8.4.0"
  },
  "engineStrict": true,
  "type": "module",
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "tsup.config.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "ts-node-esm --experimental-specifier-resolution=node ./test/index.ts",
    "pretty": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "pretty-fix": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prepare": "pnpm build && pnpm lint && pnpm pretty"
  },
  "dependencies": {
    "chalk": "5.2.0",
    "discord.js": "14.11.0",
    "glob": "10.2.2",
    "lodash": "4.17.21",
    "superjson": "1.12.3"
  },
  "devDependencies": {
    "@types/lodash": "4.14.194",
    "@types/node": "20.1.0",
    "dotenv": "16.0.3",
    "ts-node": "10.9.1"
  }
}
