  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/lodash": "4.14.182",
    "dotenv": "16.0.1",
    "ts-node-dev": "^2.0.0",
    "tsup": "^6.2.1"
  },
  "packageManager": "yarn@3.2.2"
}
