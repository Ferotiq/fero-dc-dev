{
  "name": "@ferod/client",
  "description": "An extension on Discord.JS that makes it simpler and safer to create Discord bots.",
  "version": "0.2.1",
  "author": "Evan <evanrayweigel@outlook.com>",
  "repository": "https://github.com/ferotiq/ferod",
  "bugs": {
    "url": "https://github.com/ferotiq/ferod/issues"
  },
  "homepage": "https://github.com/ferotiq/ferod#readme",
  "license": "MIT",
  "engines": {
    "node": "18.16.0",
    "bun": "1.2.0"
  },
  "engineStrict": true,
  "type": "module",
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "tsup.config.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "ts-node-esm ./test/index.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prepare": "bun run build && bun run lint"
  },
  "dependencies": {
    "chalk": "5.2.0",
    "discord.js": "14.11.0",
    "glob": "10.2.2",
    "remeda": "1.14.0"
  },
  "devDependencies": {
    "ts-node": "10.9.1"
  }
}
