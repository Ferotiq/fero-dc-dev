{
  "name": "fero-dc",
  "version": "4.0.0-dev-7",
  "description": "Ultimate Classes and Structures to organize the client, commands, events, and more for your discord bot.",
  "author": "Evan <evanrayweigel@outlook.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "ts-node-dev ./test/index.ts --extensions \"ts,json\" --watch \"src,test\"",
