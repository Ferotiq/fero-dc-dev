{
  "name": "fero-dc",
  "version": "3.0.5",
  "type": "commonjs",
  "types": "./Typings/index.d.ts",
  "description": "Ultimate Classes and Structures to organize the client, commands, events, and more for your discord bot.",
  "main": "dist/index.js",
  "repository": "https://github.com/ferotiq/fero-dc",
  "author": "Ferotiq <evanrayweigel@outlook.com>",
  "license": "MIT",
  "scripts": {
    "test": "nodemon",
    "build": "tsc && cd src/dashboard && yarn build",
    "pretty": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "pretty-fix": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "publish": "yarn run lint-fix && yarn build && npm publish"
  },
  "dependencies": {
    "discord.js": "13.6.0",
    "glob": "7.2.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/lodash": "4.14.178",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.6.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "yarn": "^1.22.18"
  }
}
