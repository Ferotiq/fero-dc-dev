{
	"compilerOptions": {
		"module": "NodeNext",
		"target": "ESNext",
		"lib": ["ESNext"],
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"removeComments": true,
		"newLine": "lf",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules"]
}
